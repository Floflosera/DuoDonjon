{
  "dialog_1" : {"textHarry": "", "spHarry": "0", "textFlaux": "", "spFlaux": "0", "cadreN": "STOP ! STOOOOP ! On arrête tout !", "spLucy": "5", "supOther": "true", "devent": "0"},
  "dialog_2" : {"textHarry": "", "spHarry": "0", "textFlaux": "", "spFlaux": "0", "cadreN": "Vous êtes forts, tous les deux. J'aimerais bien pas ressortir d'ici dans les vapes.", "spLucy": "8", "supOther": "true", "devent": "0"},
  "dialog_3" : {"textHarry": "", "spHarry": "0", "textFlaux": "... On a gagné ? À MOI LA FAUX !", "spFlaux": "1", "cadreN": "", "spLucy": "8", "supOther": "true", "devent": "0"},
  "dialog_4" : {"textHarry": "", "spHarry": "0", "textFlaux": "", "spFlaux": "1", "cadreN": "Euh, ouais, tiens, prends la.", "spLucy": "8", "supOther": "true", "devent": "0"},
  "dialog_5" : {"textHarry": "", "spHarry": "0", "textFlaux": "WOAAHHHHH ! Elle est tellement bien !", "spFlaux": "1", "cadreN": "", "spLucy": "8", "supOther": "true", "devent": "0"},
  "dialog_6" : {"textHarry": "", "spHarry": "0", "textFlaux": "", "spFlaux": "1", "cadreN": "C'est quoi son délire avec les faux ?", "spLucy": "3", "supOther": "true", "devent": "0"},
  "dialog_7" : {"textHarry": "C'est compliqué. Tu comptes nous laisser tout le trésor ?", "spHarry": "0", "textFlaux": "", "spFlaux": "1", "cadreN": "", "spLucy": "3", "supOther": "true", "devent": "0"},
  "dialog_8" : {"textHarry": "", "spHarry": "0", "textFlaux": "", "spFlaux": "1", "cadreN": "J'sais que même si je refusais, tu me laisserais pas repartir sans au moins un truc ou deux.", "spLucy": "3", "supOther": "true", "devent": "0"},
  "dialog_9" : {"textHarry": "", "spHarry": "0", "textFlaux": "", "spFlaux": "1", "cadreN": "J'vais me servir et je m'en vais. On se revoit plus tard !", "spLucy": "1", "supOther": "true", "devent": "0"},
  "dialog_10" : {"textHarry": "C'est ça. À plus.", "spHarry": "1", "textFlaux": "", "spFlaux": "1", "cadreN": "", "spLucy": "3", "supOther": "true", "devent": "0"},
  "dialog_11" : {"textHarry": "", "spHarry": "1", "textFlaux": "", "spFlaux": "1", "cadreN": "(Lucy quitte la pièce par le plafond détruit.)", "spLucy": "9", "supOther": "true", "devent": "0"},
  "dialog_12" : {"textHarry": "", "spHarry": "1", "textFlaux": "Harry, il faut que tu voies ç- Hein ? Où elle est passée ?", "spFlaux": "0", "cadreN": "", "spLucy": "9", "supOther": "true", "devent": "0"},
  "dialog_13" : {"textHarry": "", "spHarry": "0", "textFlaux": "", "spFlaux": "0", "cadreN": "J'suis partiiiiiiie !", "spLucy": "9", "supOther": "true", "devent": "0"},
  "dialog_14" : {"textHarry": "", "spHarry": "0", "textFlaux": "Oh.", "spFlaux": "0", "cadreN": "", "spLucy": "9", "supOther": "true", "devent": "0"},
  "dialog_15" : {"textHarry": "", "spHarry": "0", "textFlaux": "Bah, pas grave ! À nous le trésor !", "spFlaux": "1", "cadreN": "", "spLucy": "9", "supOther": "true", "devent": "0"},
  "dialog_16" : {"textHarry": "À nous le trésor. Bien joué, partenaire.", "spHarry": "1", "textFlaux": "", "spFlaux": "0", "cadreN": "", "spLucy": "9", "supOther": "true", "devent": "0"},
  "dialog_17" : {"textHarry": "", "spHarry": "1", "textFlaux": "Ouais, ouais. Allez, viens voir !", "spFlaux": "0", "cadreN": "", "spLucy": "9", "supOther": "true", "devent": "0"},
