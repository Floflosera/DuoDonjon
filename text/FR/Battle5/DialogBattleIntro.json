{
  "dialog_1" : {"textHarry": "", "spHarry": "0", "textFlaux": "", "spFlaux": "0", "cadreN": "Ouah, ça a l'air précieux, ça...", "spLucy": "2", "supOther": "true", "devent": "0"},
  "dialog_2" : {"textHarry": "Pincez-moi, je rêve...", "spHarry": "3", "textFlaux": "", "spFlaux": "0", "cadreN": "", "spLucy": "2", "supOther": "true", "devent": "0"},
  "dialog_3" : {"textHarry": "", "spHarry": "3", "textFlaux": "", "spFlaux": "0", "cadreN": "HARRY ?! Mais qu'est-ce que tu fais là ? Et c'est qui, elle ?", "spLucy": "4", "supOther": "true", "devent": "0"},
  "dialog_4" : {"textHarry": "", "spHarry": "3", "textFlaux": "OH CETTE FAUX EST TROP BELLE QU'EST-CE QUE QUOI-", "spFlaux": "1", "cadreN": "", "spLucy": "4", "supOther": "true", "devent": "0"},
  "dialog_5" : {"textHarry": "", "spHarry": "3", "textFlaux": "*tousse*", "spFlaux": "5", "cadreN": "", "spLucy": "4", "supOther": "true", "devent": "0"},
  "dialog_6" : {"textHarry": "", "spHarry": "0", "textFlaux": "Euh... Tu la connais ?", "spFlaux": "0", "cadreN": "", "spLucy": "0", "supOther": "true", "devent": "0"},
  "dialog_7" : {"textHarry": "C'est... une amie. Elle s'appelle Lucy. ", "spHarry": "0", "textFlaux": "", "spFlaux": "0", "cadreN": "", "spLucy": "0", "supOther": "true", "devent": "0"},
  "dialog_8" : {"textHarry": "Et, Lucy, je te présente Flaux. Je l'ai rencontrée dans ce donjon.", "spHarry": "0", "textFlaux": "", "spFlaux": "0", "cadreN": "", "spLucy": "0", "supOther": "true", "devent": "0"},
  "dialog_9" : {"textHarry": "", "spHarry": "0", "textFlaux": "", "spFlaux": "0", "cadreN": "Donjon ? C'est pas juste un genre de cachette secrète ? Mince alors, j'ai raté tous les trucs à taper.", "spLucy": "5", "supOther": "true", "devent": "0"},
  "dialog_10" : {"textHarry": "Elle... aime bien se battre. J'imagine qu'elle a fait exploser le plafond pour arriver ici.", "spHarry": "0", "textFlaux": "", "spFlaux": "0", "cadreN": "", "spLucy": "5", "supOther": "true", "devent": "0"},
  "dialog_11" : {"textHarry": "", "spHarry": "0", "textFlaux": "", "spFlaux": "0", "cadreN": "C'était un accident !", "spLucy": "4", "supOther": "true", "devent": "0"},
  "dialog_12" : {"textHarry": "", "spHarry": "0", "textFlaux": "Bon ! Au moins, ça nous fait un raccourci pour sortir de là ! On prend le trésor et on s'en va !", "spFlaux": "1", "cadreN": "", "spLucy": "8", "supOther": "true", "devent": "0"},
  "dialog_13" : {"textHarry": "", "spHarry": "0", "textFlaux": "", "spFlaux": "1", "cadreN": "Hep hep hep ! Hors de question. J'ai trouvé ça en premier, il est à moi !", "spLucy": "0", "supOther": "true", "devent": "0"},
  "dialog_14" : {"textHarry": "", "spHarry": "0", "textFlaux": "Pardon ? Après tout ce qu'on a traversé pour arriver là ? Tu pourrais au moins partager !", "spFlaux": "2", "cadreN": "", "spLucy": "0", "supOther": "true", "devent": "0"},
  "dialog_15" : {"textHarry": "Ça va mal finir...", "spHarry": "3", "textFlaux": "", "spFlaux": "2", "cadreN": "", "spLucy": "0", "supOther": "true", "devent": "0"},
  "dialog_16" : {"textHarry": "", "spHarry": "3", "textFlaux": "", "spFlaux": "2", "cadreN": "Vous le voulez ? Faudra d'abord me passer sur le corps !", "spLucy": "0", "supOther": "true", "devent": "0"},
  "dialog_17" : {"textHarry": "Il fallait s'en douter.", "spHarry": "3", "textFlaux": "", "spFlaux": "2", "cadreN": "", "spLucy": "0", "supOther": "true", "devent": "0"},
  "dialog_18" : {"textHarry": "", "spHarry": "3", "textFlaux": "Aucun problème ! Amène-toi !", "spFlaux": "2", "cadreN": "", "spLucy": "0", "supOther": "true", "devent": "0"},
}