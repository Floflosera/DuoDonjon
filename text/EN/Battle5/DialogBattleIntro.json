{
  "dialog_1" : {"textHarry": "", "spHarry": "0", "textFlaux": "", "spFlaux": "0", "cadreN": "Woah, that looks valuable...", "spLucy": "2", "supOther": "true", "devent": "0"},
  "dialog_2" : {"textHarry": "No way, tell me I’m dreaming...", "spHarry": "3", "textFlaux": "", "spFlaux": "0", "cadreN": "", "spLucy": "2", "supOther": "true", "devent": "0"},
  "dialog_3" : {"textHarry": "", "spHarry": "3", "textFlaux": "", "spFlaux": "0", "cadreN": "HARRY?! What’re you doing here? And who’s that girl?", "spLucy": "4", "supOther": "true", "devent": "0"},
  "dialog_4" : {"textHarry": "", "spHarry": "3", "textFlaux": "WOAH LOOK AT THAT SCYTHE IT’S SO BEAUTIFUL WHA-", "spFlaux": "1", "cadreN": "", "spLucy": "4", "supOther": "true", "devent": "0"},
  "dialog_5" : {"textHarry": "", "spHarry": "3", "textFlaux": "*cough*", "spFlaux": "5", "cadreN": "", "spLucy": "4", "supOther": "true", "devent": "0"},
  "dialog_6" : {"textHarry": "", "spHarry": "0", "textFlaux": "Erm... You know her?", "spFlaux": "0", "cadreN": "", "spLucy": "3", "supOther": "true", "devent": "0"},
  "dialog_7" : {"textHarry": "She’s... a friend. Her name is Lucy.", "spHarry": "0", "textFlaux": "", "spFlaux": "0", "cadreN": "", "spLucy": "3", "supOther": "true", "devent": "0"},
  "dialog_8" : {"textHarry": "And, Lucy, this is Flaux. I met her in this dungeon.", "spHarry": "0", "textFlaux": "", "spFlaux": "0", "cadreN": "", "spLucy": "3", "supOther": "true", "devent": "0"},
  "dialog_9" : {"textHarry": "", "spHarry": "0", "textFlaux": "", "spFlaux": "0", "cadreN": "Dungeon? So it’s not a hidden cache or something? Shoot, I missed all the stuff to beat up.", "spLucy": "5", "supOther": "true", "devent": "0"},
  "dialog_10" : {"textHarry": "She... likes to fight. I’m guessing she blew up the ceiling to get here.", "spHarry": "0", "textFlaux": "", "spFlaux": "0", "cadreN": "", "spLucy": "5", "supOther": "true", "devent": "0"},
  "dialog_11" : {"textHarry": "", "spHarry": "0", "textFlaux": "", "spFlaux": "0", "cadreN": "T’was an accident!", "spLucy": "4", "supOther": "true", "devent": "0"},
  "dialog_12" : {"textHarry": "", "spHarry": "0", "textFlaux": "Anyway! At least, we got a shortcut to get out! We take the treasure and we leave this place!", "spFlaux": "1", "cadreN": "", "spLucy": "4", "supOther": "true", "devent": "0"},
  "dialog_13" : {"textHarry": "", "spHarry": "0", "textFlaux": "", "spFlaux": "1", "cadreN": "Hey hey hey! No way I’m letting you have it. I found it first!", "spLucy": "0", "supOther": "true", "devent": "0"},
  "dialog_14" : {"textHarry": "", "spHarry": "0", "textFlaux": "‘Scuse me? After everything we went through to get here? You could at least share!", "spFlaux": "2", "cadreN": "", "spLucy": "0", "supOther": "true", "devent": "0"},
  "dialog_15" : {"textHarry": "This is not going to end well...", "spHarry": "3", "textFlaux": "", "spFlaux": "2", "cadreN": "", "spLucy": "0", "supOther": "true", "devent": "0"},
  "dialog_16" : {"textHarry": "", "spHarry": "3", "textFlaux": "", "spFlaux": "2", "cadreN": "You want it? You’ll have to take it from me!", "spLucy": "0", "supOther": "true", "devent": "0"},
  "dialog_17" : {"textHarry": "Called it.", "spHarry": "3", "textFlaux": "", "spFlaux": "2", "cadreN": "", "spLucy": "0", "supOther": "true", "devent": "0"},
  "dialog_18" : {"textHarry": "", "spHarry": "3", "textFlaux": "No problem! Come on!", "spFlaux": "2", "cadreN": "", "spLucy": "0", "supOther": "true", "devent": "0"},
}