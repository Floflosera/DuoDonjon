{
	"dialog_1" : {"textHarry": "There we go.", "spHarry": "1", "textFlaux": "", "spFlaux": "0", "cadreN": "", "supOther": "true", "devent": "2"},
	"dialog_2" : {"textHarry": "", "spHarry": "1", "textFlaux": "Good riddance! Now let me out of that cage!", "spFlaux": "1", "cadreN": "", "supOther": "true", "devent": "0"},
	"dialog_3" : {"textHarry": "Wait a moment.", "spHarry": "2", "textFlaux": "", "spFlaux": "1", "cadreN": "", "supOther": "true", "devent": "0"},
	"dialog_4" : {"textHarry": "", "spHarry": "2", "textFlaux": "Hm ?", "spFlaux": "0", "cadreN": "", "supOther": "true", "devent": "0"},
	"dialog_5" : {"textHarry": "You’re going to have to answer a couple of questions first. Who are you and what are you doing here?", "spHarry": "2", "textFlaux": "", "spFlaux": "0", "cadreN": "", "supOther": "true", "devent": "0"},
	"dialog_6" : {"textHarry": "", "spHarry": "2", "textFlaux": "That’s none of your business, is it?", "spFlaux": "2", "cadreN": "", "supOther": "true", "devent": "0"},
	"dialog_7" : {"textHarry": "You’re right. So, I’ll see you when I leave? I’m going to go further inside this dungeon.", "spHarry": "0", "textFlaux": "", "spFlaux": "2", "cadreN": "", "supOther": "true", "devent": "0"},
	"dialog_8" : {"textHarry": "", "spHarry": "0", "textFlaux": "WAIT! Wait! Alright! My name’s Flaux, and I’m a treasu- an explorer. That robot attacked me on scythe and managed to knock me out, and I ended up here. There, happy?", "spFlaux": "4", "cadreN": "", "supOther": "true", "devent": "0"},
	"dialog_9" : {"textHarry": "Yeah.", "spHarry": "1", "textFlaux": "", "spFlaux": "4", "cadreN": "", "supOther": "true", "devent": "0"},
	"dialog_10" : {"textHarry": "(Scythe...? Sight?)", "spHarry": "0", "textFlaux": "", "spFlaux": "4", "cadreN": "", "supOther": "true", "devent": "0"},
	"dialog_11" : {"textHarry": "", "spHarry": "1", "textFlaux": "Okay, cool! Now open that cage!", "spFlaux": "0", "cadreN": "", "supOther": "true", "devent": "0"},
	"dialog_12" : {"textHarry": "If you want, I could accompany you, and we’d split everything we find 50/50… Just to prevent more \"accidents\".", "spHarry": "1", "textFlaux": "", "spFlaux": "0", "cadreN": "", "supOther": "true", "devent": "0"},
	"dialog_13" : {"textHarry": "", "spHarry": "1", "textFlaux": "... You’ve got no shame. Deal.", "spFlaux": "2", "cadreN": "", "supOther": "true", "devent": ""},
	"dialog_14" : {"textHarry": "", "spHarry": "0", "textFlaux": "I’m freeeeeeee! Ah, how I missed my scythe!", "spFlaux": "1", "cadreN": "", "supOther": "true", "devent": "2"},
	"dialog_15" : {"textHarry": "You fight with a scythe?", "spHarry": "0", "textFlaux": "", "spFlaux": "1", "cadreN": "", "supOther": "true", "devent": "0"},
	"dialog_16" : {"textHarry": "", "spHarry": "0", "textFlaux": "What’s wrong with that? You fight with a shield. Don’t think it’s that efficient.", "spFlaux": "0", "cadreN": "", "supOther": "true", "devent": "0"},
	"dialog_17" : {"textHarry": "If you say so. Should we get going?", "spHarry": "0", "textFlaux": "", "spFlaux": "0", "cadreN": "", "supOther": "true", "devent": "0"},
	"dialog_18" : {"textHarry": "", "spHarry": "0", "textFlaux": "Yeah, let’s go.", "spFlaux": "0", "cadreN": "", "supOther": "true", "devent": "0"},
}