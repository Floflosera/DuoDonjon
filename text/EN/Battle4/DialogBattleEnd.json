{
  "dialog_1" : {"textHarry": "Phew... That was close.", "spHarry": "5", "textFlaux": "", "spFlaux": "5", "cadreN": "", "supOther": "true", "devent": "0"},
  "dialog_2" : {"textHarry": "", "spHarry": "5", "textFlaux": "I admit I got a little worried there.", "spFlaux": "5", "cadreN": "", "supOther": "true", "devent": "0"},
  "dialog_3" : {"textHarry": "...", "spHarry": "0", "textFlaux": "", "spFlaux": "0", "cadreN": "", "supOther": "true", "devent": "0"},
  "dialog_4" : {"textHarry": "", "spHarry": "0", "textFlaux": "...", "spFlaux": "0", "cadreN": "", "supOther": "", "devent": "0"},
  "dialog_5" : {"textHarry": "\"My friend\"?", "spHarry": "1", "textFlaux": "\"My friend\"?", "spFlaux": "2", "cadreN": "", "supOther": "true", "devent": "0"},
  "dialog_6" : {"textHarry": "Heh, I guess we’re getting along, in the end.", "spHarry": "1", "textFlaux": "", "spFlaux": "2", "cadreN": "", "supOther": "true", "devent": "0"},
  "dialog_7" : {"textHarry": "", "spHarry": "1", "textFlaux": "...", "spFlaux": "2", "cadreN": "", "supOther": "true", "devent": "0"},
  "dialog_8" : {"textHarry": "", "spHarry": "1", "textFlaux": "......", "spFlaux": "0", "cadreN": "", "supOther": "true", "devent": "0"},
  "dialog_9" : {"textHarry": "", "spHarry": "1", "textFlaux": "Okay, you win. I wouldn’t have made it here if you hadn’t been with me, and I guess you’re pretty good at what you do.", "spFlaux": "0", "cadreN": "", "supOther": "true", "devent": "0"},
  "dialog_10" : {"textHarry": "", "spHarry": "1", "textFlaux": "You saved my skin several times already, and I’m grateful for that. So, yeah, you can be a friend.", "spFlaux": "0", "cadreN": "", "supOther": "true", "devent": "0"},
  "dialog_11" : {"textHarry": "That’s good to hear. I’m happy you’re here with me, because I wouldn’t have done much by myself either.", "spHarry": "1", "textFlaux": "", "spFlaux": "0", "cadreN": "", "supOther": "true", "devent": "0"},
  "dialog_12" : {"textHarry": "I’m too weak by myself… But I’m sure that together, we can achieve greater things. I’m happy to count you as one of my friends.", "spHarry": "1", "textFlaux": "", "spFlaux": "0", "cadreN": "", "supOther": "true", "devent": "0"},
  "dialog_13" : {"textHarry": "", "spHarry": "1", "textFlaux": "Yeah, yeah, why not. You’re embarrassing. Let’s move.", "spFlaux": "0", "cadreN": "", "supOther": "true", "devent": "0"},
  "dialog_14" : {"textHarry": "Let’s go.", "spHarry": "1", "textFlaux": "", "spFlaux": "0", "cadreN": "", "supOther": "true", "devent": "0"},
  "dialog_15" : {"textHarry": "", "spHarry": "4", "textFlaux": "", "spFlaux": "4", "cadreN": "*CRASH* *BROMBROLOM*", "supOther": "true", "devent": "0"},
  "dialog_16" : {"textHarry": "", "spHarry": "4", "textFlaux": "What was that?!", "spFlaux": "4", "cadreN": "", "supOther": "true", "devent": "0"},
  "dialog_17" : {"textHarry": "Looks like it came from the next room. Stay on your guard.", "spHarry": "2", "textFlaux": "", "spFlaux": "4", "cadreN": "", "supOther": "true", "devent": "0"},
